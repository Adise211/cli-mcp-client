{
  "name": "cli-mcp-client",
  "version": "1.0.0",
  "description": "A TypeScript client for the Model Context Protocol (MCP) that enables seamless integration between Anthropic's Claude AI and MCP servers",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && chmod 755 build/index.js",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "anthropic",
    "claude",
    "ai",
    "typescript",
    "node",
    "tools",
    "integration"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.1",
    "@modelcontextprotocol/sdk": "^1.18.1",
    "dotenv": "^17.2.2",
    "winston": "^3.17.0"
  }
}
